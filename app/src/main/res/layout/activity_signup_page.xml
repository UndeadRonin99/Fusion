<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignupPage">

    <ImageView
        android:id="@+id/image_logo"
        android:layout_width="564dp"
        android:layout_height="846dp"
        android:scaleType="centerInside"
        android:src="@drawable/image_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.076" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="41dp"
        android:layout_marginTop="259dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="155dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.754"
        app:srcCompat="@drawable/image_view_rectanglesignup" />

    <Button
        android:id="@+id/btnSignupPage"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginTop="269dp"
        android:layout_marginEnd="221dp"
        android:layout_marginBottom="517dp"
        android:background="#00FFFFFF"
        android:text="Signup"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/image_logo"
        app:layout_constraintEnd_toEndOf="@+id/image_logo"
        app:layout_constraintTop_toTopOf="@+id/image_logo" />

    <Button
        android:id="@+id/BtnLoginPage"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginStart="83dp"
        android:layout_marginTop="269dp"
        android:layout_marginBottom="517dp"
        android:background="#00FFFFFF"
        android:text="Login"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/image_logo"
        app:layout_constraintStart_toStartOf="@+id/image_logo"
        app:layout_constraintTop_toTopOf="@+id/image_logo" />

    <ImageView
        android:id="@+id/image_background"
        android:layout_width="227dp"
        android:layout_height="202dp"
        android:layout_marginStart="103dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="234dp"
        android:layout_marginBottom="600dp"
        android:scaleType="centerInside"
        android:src="@drawable/image_background"
        app:layout_constraintBottom_toBottomOf="@+id/image_logo"
        app:layout_constraintEnd_toEndOf="@+id/image_logo"
        app:layout_constraintStart_toStartOf="@+id/image_logo"
        app:layout_constraintTop_toTopOf="@+id/image_logo" />

    <FrameLayout
        android:id="@+id/container_button"
        android:layout_width="224dp"
        android:layout_height="50dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="761dp"
        android:layout_marginEnd="236dp"
        android:layout_marginBottom="35dp"
        android:background="@drawable/container_button"
        app:layout_constraintBottom_toBottomOf="@+id/image_logo"
        app:layout_constraintEnd_toEndOf="@+id/image_logo"
        app:layout_constraintStart_toStartOf="@+id/image_logo"
        app:layout_constraintTop_toTopOf="@+id/image_logo">

        <Button
            android:id="@+id/button"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#00FFFFFF"
            android:text="Signup"
            android:textColor="#000000"
            android:textSize="20sp" />
    </FrameLayout>

    <EditText
        android:id="@+id/txtLUsername"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="270dp"
        android:background="@drawable/image_view_rectangle1"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/edtEmail"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="220dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="180dp"
        android:background="@drawable/image_view_rectangle1"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/txtLPassword"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="312dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="88dp"
        android:background="@drawable/image_view_rectangle1"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/edtCPassword"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="396dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/image_view_rectangle2"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/container_pass"
        android:layout_width="160dp"
        android:layout_height="24dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="607dp"
        android:layout_marginEnd="119dp"
        android:layout_marginBottom="34dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="Confirm Password"
        android:textColor="#FF000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/edtCPassword"
        app:layout_constraintEnd_toEndOf="@+id/edtCPassword"
        app:layout_constraintStart_toStartOf="@+id/edtCPassword"
        app:layout_constraintTop_toTopOf="@+id/image_logo" />

    <TextView
        android:id="@+id/text_email"
        android:layout_width="51dp"
        android:layout_height="22dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="209dp"
        android:layout_marginEnd="425dp"
        android:layout_marginBottom="388dp"
        android:alpha="0.800000011920929"
        android:gravity="left|center_vertical"
        android:text="Email"
        android:textColor="#FF000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/image_logo"
        app:layout_constraintEnd_toEndOf="@+id/image_logo"
        app:layout_constraintStart_toStartOf="@+id/image_logo"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/container_pass1"
        android:layout_width="93dp"
        android:layout_height="25dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="118dp"
        android:layout_marginEnd="217dp"
        android:layout_marginBottom="307dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="Username"
        android:textColor="#FF000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="86dp"
        android:layout_height="23dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="299dp"
        android:layout_marginEnd="217dp"
        android:layout_marginBottom="128dp"
        android:text="Password"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

</androidx.constraintlayout.widget.ConstraintLayout>
